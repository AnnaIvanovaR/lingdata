    <TIER ANNOTATOR="@AnnaIvanovaR" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@gas1929f" TIER_ID="trans@babushka">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Hello, hello!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Hello!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>I'm Goreshenko Antonina Seregeevna.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>I was born in 1929 on the Screplevo state farm.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>And since then I have been living in the village, in the countryside.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>I worked very hard, very, very hard.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>I'm 83 now, yeah.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>And I still worry when I see the dirty rolls lying around the farm.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>There was no such thing before, the way we worked.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>We worked day and night.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>I have milked cows for 25 years.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>I was the oldest in the family, yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>II was the oldest, I did all the work.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>Ah, at that time they worked not for money, but for working days.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>We got some bread ... (untranslatable)</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Well, there were many of us children.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>They will bake some loaf, well, eat it like that.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="@AnnaIvanovaR" DEFAULT_LOCALE="en"
